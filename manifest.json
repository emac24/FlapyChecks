{
    "manifest_version": 3,
    "name": "Flapy Capy Checks",
    "version": "1.0",
    "description": "Meet \"Flapy\", your personal finance bro. (who's also a capybara)",
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {   
            "16":"Cappy-icon16.png",
            "48": "Cappy-icon48.png",
            "128": "Cappy-icon128.png"
        }
    },

    "background": {
        "service_worker": "popup.js"
    },
    "icons":{
        "16":"Cappy-icon16.png",
        "48": "Cappy-icon48.png",
        "128": "Cappy-icon128.png"
    }

}